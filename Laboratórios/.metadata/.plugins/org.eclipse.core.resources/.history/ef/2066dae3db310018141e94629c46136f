		super(id, nome, descricao);
	}

	public void adicionarMembro(Usuario usuario) {
		// caronas.add(usuario);
	}

	// Método toString:
	@Override
	public String toString() {
		String out = "GrupoPrivado\n";
		out += "- id: " + getId() + "\n";
		out += "- nome: " + getNome() + "\n";
