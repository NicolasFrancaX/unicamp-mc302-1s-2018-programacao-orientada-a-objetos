	public void adicionarMembro(Usuario usuario) {
		// caronas.add(usuario);
	}

	// Método toString:
	@Override
	public String toString() {
		String out = "GrupoPrivado\n";
		out += "- id: " + this.id + "\n";
		out += "- nome: " + this.nome + "\n";
		out += "- descrição: " + this.descricao + "\n";
		out += "- membros: " + this.membros + "\n";
		return out;
	}
}
